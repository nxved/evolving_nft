        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));

        bool canSwap = contractTokenBalance >= swapTokensAtAmount;

        if (
            canSwap &&
            !swapping &&
            !automatedMarketMakerPairs[from] &&
            from != owner()
        ) {
            swapping = true;
            swapTokensForUSDTFEE(contractTokenBalance);
            swapping = false;
        }

        uint256 usdtBalance = IBEP20(USDT).balanceOf(address(this));

        bool canBurnAndSend = block.timestamp > nextBuyBack;

        if (
            canBurnAndSend &&
            !sending &&
            !automatedMarketMakerPairs[from] &&
            usdtBalance > 0 &&
            from != owner()
        ) {
            sending = true;
            (uint256 buybackFee, uint256 feePool) = calculateFee(usdtBalance);

            if (buybackFee > 0) {
                swapTokensForGKHANFEE(buybackFee);
                _burn(address(this), buybackFee);
            }

            if (feePool > 0) {
                IBEP20(USDT).transfer(feesPool, feePool);
            }

            sending = false;
        }

        bool takeFee = true;

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        uint256 finalAmount = amount;

        if (takeFee && automatedMarketMakerPairs[to]) {
            uint256 fees = calculateSalesTax(amount, from);
            finalAmount = amount - fees;
            if (fees > 0) {
                super._transfer(from, address(this), fees);
            }
        }

        super._transfer(from, to, finalAmount);
    }
}
